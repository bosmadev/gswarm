{
	"name": "gswarm",
	"displayName": "GSwarm",
	"description": "Gemini Swarm API Proxy",
	"version": "1.0.11",
	"publisher": "bosmadev",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/bosmadev/gswarm"
	},
	"type": "module",
	"scripts": {
		"############ SERVER ############": "",
		"dev": "dotenvx run -- node --input-type=commonjs -e \"process.env.PORT=process.env.GLOBAL_PORT||'3000';require('child_process').execSync('next dev --turbopack',{stdio:'inherit',shell:true})\"",
		"dev:debug": "node --experimental-transform-types scripts/launch.ts --debug",
		"start": "dotenvx run -- node --input-type=commonjs -e \"process.env.PORT=process.env.GLOBAL_PORT||'3000';require('child_process').execSync('next start',{stdio:'inherit',shell:true})\"",
		"launch": "node --experimental-transform-types scripts/launch.ts",
		"launch:debug": "DEBUG=true node --experimental-transform-types scripts/launch.ts",
		"launch:tunnel": "cloudflared tunnel run $npm_package_name",
		"gswarm": "GSWARM_CLI=true tsx lib/gswarm/cli.ts",
		"############ LINTING & VALIDATION ############": "",
		"knip": "knip --no-config-hints",
		"knip:strict": "knip --strict",
		"tsc": "tsc --noEmit",
		"check": "biome check",
		"check:fix": "biome check --write",
		"check:all": "biome check .",
		"check:all:fix": "biome check . --write",
		"check:strict": "biome check . --error-on-warnings",
		"check:unsafe": "biome check . --apply-suggested --unsafe",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"lint:all": "biome lint .",
		"lint:all:fix": "biome lint . --write",
		"format": "biome format",
		"format:write": "biome format --write",
		"format:all": "biome format .",
		"format:all:fix": "biome format . --write",
		"############ TESTING ############": "",
		"vitest:run": "vitest run",
		"vitest:ui": "vitest --ui",
		"pytest": "cd python && uv run pytest",
		"############ BUILDING ############": "",
		"tw:upgrade": "pnpm dlx @tailwindcss/upgrade@^4.0.0",
		"validate": "(pnpm tw:upgrade || echo [WARN] Tailwind upgrade skipped) && pnpm knip && pnpm check:all && pnpm tsc && pnpm vitest:run && pnpm pytest",
		"version:sync": "node --experimental-transform-types python/sync-pyproject.ts",
		"build": "pnpm version:sync && pnpm rimraf:next && pnpm validate && next build",
		"postbuild": "node --experimental-transform-types scripts/launch.ts postbuild",
		"############ OTHER ############": "",
		"reinstall": "node --experimental-transform-types scripts/launch.ts reinstall",
		"rimraf:next": "rimraf .next node_modules/.cache",
		"env:encrypt": "dotenvx encrypt",
		"env:decrypt": "dotenvx decrypt",
		"env:check": "node -e \"const f=require('fs').readFileSync('.env','utf8'); process.exit(f.includes('encrypted:') ? (console.log('✅ .env encrypted'),0) : (console.log('❌ .env NOT encrypted'),1))\""
	},
	"engines": {
		"node": ">=22.6.0"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@dotenvx/dotenvx": "^1.51.4",
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-avatar": "^1.1.11",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-collapsible": "^1.1.12",
		"@radix-ui/react-context-menu": "^2.2.16",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-hover-card": "^1.1.15",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-navigation-menu": "^1.2.14",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-radio-group": "^1.3.8",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slider": "^1.3.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-toggle": "^1.1.10",
		"@radix-ui/react-toggle-group": "^1.1.11",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tanstack/react-virtual": "^3.13.18",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"ioredis": "^5.9.3",
		"lucide-react": "^0.562.0",
		"mermaid": "^11.12.2",
		"next": "^16.1.2",
		"node-cron": "^4.2.1",
		"react": "^19.2.3",
		"react-day-picker": "^9.13.0",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.71.1",
		"react-markdown": "^10.1.0",
		"recharts": "^3.7.0",
		"rehype-raw": "^7.0.0",
		"remark-gfm": "^4.0.1",
		"shiki": "^3.21.0",
		"sonner": "^2.0.7",
		"swr": "^2.3.8",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/node": "^25.0.3",
		"@types/node-cron": "^3.0.11",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.16",
		"autoprefixer": "^10.4.23",
		"babel-plugin-react-compiler": "^1.0.0",
		"happy-dom": "^20.0.11",
		"knip": "^5.77.1",
		"postcss": "^8.5.6",
		"postcss-load-config": "^6.0.1",
		"rimraf": "^6.1.2",
		"tailwindcss": "^4.1.18",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.20.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "19",
				"react-dom": "19"
			}
		}
	},
	"overrides": {
		"glob": "^10.4.5"
	},
	"knip": {
		"ignoreExportsUsedInFile": true,
		"ignoreBinaries": [
			"uv",
			"cloudflared"
		],
		"ignore": [
			"components/ui/**",
			"scripts/launch.ts",
			"scripts/migrate-to-redis.ts",
			"scripts/populate-token-projects.ts",
			"scripts/run-integration-tests.ts",
			"lib/console.ts",
			"lib/gswarm/storage/**",
			"lib/gswarm/schemas.ts",
			"lib/gswarm/client.ts",
			"lib/gswarm/errors.ts",
			"lib/gswarm/projects.ts",
			"lib/gswarm/oauth.ts",
			"scripts/reactgrab-patch.cjs",
			"app/api/gswarm/_shared/streaming.ts"
		],
		"ignoreDependencies": [
			"zod",
			"@dnd-kit/core",
			"@dnd-kit/sortable",
			"@dnd-kit/utilities",
			"@hookform/resolvers",
			"@radix-ui/react-accordion",
			"@radix-ui/react-alert-dialog",
			"@radix-ui/react-avatar",
			"@radix-ui/react-checkbox",
			"@radix-ui/react-collapsible",
			"@radix-ui/react-context-menu",
			"@radix-ui/react-dialog",
			"@radix-ui/react-dropdown-menu",
			"@radix-ui/react-hover-card",
			"@radix-ui/react-icons",
			"@radix-ui/react-label",
			"@radix-ui/react-navigation-menu",
			"@radix-ui/react-popover",
			"@radix-ui/react-radio-group",
			"@radix-ui/react-scroll-area",
			"@radix-ui/react-select",
			"@radix-ui/react-separator",
			"@radix-ui/react-slider",
			"@radix-ui/react-slot",
			"@radix-ui/react-switch",
			"@radix-ui/react-tabs",
			"@radix-ui/react-toggle",
			"@radix-ui/react-toggle-group",
			"@radix-ui/react-tooltip",
			"@tanstack/react-virtual",
			"class-variance-authority",
			"date-fns",
			"react-day-picker",
			"react-hook-form",
			"mermaid",
			"shiki",
			"clsx",
			"tailwind-merge",
			"node-cron",
			"react-markdown",
			"rehype-raw",
			"remark-gfm",
			"tailwindcss",
			"tailwindcss-animate",
			"@types/node-cron"
		],
		"rules": {
			"exports": "warn"
		}
	}
}
